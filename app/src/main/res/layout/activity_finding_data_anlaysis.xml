<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/finding_data_background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/status_bar_finding_data_anlaysis"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="@color/theme_blue"
        android:orientation="vertical"
        android:visibility="gone"></LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="0.08"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:gravity="center">

            <TextView
                android:id="@+id/pipei"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="匹配度:"
                android:textColor="@color/finding_data_text_color"

                />

            <LinearLayout
                android:id="@+id/finding_data_hobby"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pipei"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="爱好:"
                    android:drawableLeft="@mipmap/ic_finding_data_anlaysic_hobby"
                    android:drawablePadding="3dp"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_text_size" />

                <TextView
                    android:id="@+id/tv_finding_data_hobby_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.95"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_score_text_size" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/finding_data_work"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/finding_data_hobby"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="工作:"
                    android:drawableLeft="@mipmap/ic_finding_data_anlaysic_work"
                    android:drawablePadding="3dp"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_text_size" />

                <TextView
                    android:id="@+id/tv_finding_data_job_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.94"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_score_text_size" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/finding_data_live"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/finding_data_work"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="住处:"
                    android:drawableLeft="@mipmap/ic_finding_data_anlaysic_living_place"
                    android:drawablePadding="3dp"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_text_size" />

                <TextView
                    android:id="@+id/tv_finding_data_live_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.94"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_score_text_size" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/finding_data_age"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/finding_data_live"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="年龄:"
                    android:drawableLeft="@mipmap/ic_finding_data_anlaysic_age"
                    android:drawablePadding="3dp"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_text_size" />

                <TextView
                    android:id="@+id/tv_finding_data_age_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.94"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_score_text_size" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/finding_data_character"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/finding_data_age"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="性格:"
                    android:drawableLeft="@mipmap/ic_finding_data_anlaysic_character"
                    android:drawablePadding="3dp"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_text_size" />

                <TextView
                    android:id="@+id/tv_finding_data_character_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.94"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_score_text_size" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/finding_data_emotion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/finding_data_character"
                android:layout_marginTop="10dp">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@mipmap/ic_finding_data_anlaysic_mood"
                    android:drawablePadding="3dp"
                    android:text="情感:"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_text_size" />

                <TextView
                    android:id="@+id/tv_finding_data_emotion_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.94"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_score_text_size" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/finding_data_beauty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/finding_data_emotion"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="颜值:"
                    android:drawableLeft="@mipmap/ic_finding_data_anlaysic_outlooking"
                    android:drawablePadding="3dp"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_text_size" />

                <TextView
                    android:id="@+id/tv_finding_data_beauty_score"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0.94"
                    android:textColor="@color/finding_data_text_color"
                    android:textSize="@dimen/finding_data_anlaysis_pipei_score_text_size" />

            </LinearLayout>
        </RelativeLayout>

        <com.timqi.windrosediagram.WindRoseDiagramView
            android:id="@+id/windRoseDiagramView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:paddingTop="170dp"
            app:anchorColor="#302c2c"
            app:anchorWidth="4dp"
            app:foregroundColor="#881888ec"
            app:outlineColor="#cc323131"
            app:outlineWidth="1.5dp"
            app:touchSlop="24dp"
            app:windRoseBackgroundColor="#00000000"
            app:windRoseStartAngle="90"
            app:windRoseTextColor="#cc1e1c1c"
            app:windRoseTextSize="15sp" />


    </LinearLayout>
</LinearLayout>
